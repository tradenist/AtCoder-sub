   "source": [
    "n=int(input())\n",
    "a=[int(i) for i in input().split()]\n",
    "a.sort()\n",
    "\n",
    "ans=0\n",
    "for i in range(n):\n",
    "    for j in range(i+1,n):\n",
    "        ans=ans+(a[i]*a[j]*2**(j-i-1))%998244353\n",
    "ans=(ans+sum([i**2 for i in a]))%998244353\n",
    "print(ans)"
